{
  "mcpServers": {
    "pg-da": {
      "command": "uv",
      "args": [
        "--directory",
        "C:/Code/pg-da",
        "run",
        "python",
        "C:/Code/pg-da/main.py"
      ],
      "env": {
        "DATABASE_URL": "postgresql://username:password@localhost:5432/your_database"
      }
    }
  }
}